{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["member"]
    },
    {
      "route": "/app/*",
      "allowedRoles": ["member"]
    },
    {
      "route": "/login",
      "rewrite": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "rewrite": "/.auth/logout"
    },
    {
      "route": "/.auth/login/twitter",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/github",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/facebook",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/google",
      "statusCode": 404
    }
  ]
}

